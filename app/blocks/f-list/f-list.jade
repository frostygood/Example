mixin f-list(data)

	-data = data || {}
		
	if (data.mod === 1 || !data.mod)
		+b.f-list
			+e('span').title Фрилансерам:
			+e('ul')
				+e('li')
					+e(href='#').link Регистрация для фрилансеров
				+e('li')
					+e(href='#').link Общая информация
				+e('li')
					+e(href='#').link SmartCAT для фрилансеров
				+e('li')
					+e(href='#').link Как работать в SmartCAT
				+e('li')
					+e(href='#').link Работа переводчиком
				+e('li')
					+e(href='#').link Работа для фрилансеров
	if (data.mod === 2 || !data.mod)
		+b.f-list
			+e('span').title Компаниям:
			+e('ul')
				+e('li')
					+e(href='#').link Регистрация для компаний
				+e('li')
					+e(href='#').link Общая информация
				+e('li')
					+e(href='#').link Как работать в SmartCAT
				+e('li')
					+e(href='#').link Карта сайта
				+e('li')
					+e(href='#').link Вебинары
				+e('li')
					+e(href='#').link База переводчиков
